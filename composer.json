{
    "name": "ise/php-metapackage-development",
    "description": "Development metapackage for PHP used by Internalsystemerror Limited",
    "type": "metapackage",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/internalsystemerror/php-metapackage-development",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || ^7.0",
        "roave/security-advisories": "dev-master",
        "sensiolabs/security-checker": "^4.1",
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.2",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0",
        "phploc/phploc": "^4.0",
        "theseer/phpdox": "^0.11 || ^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.1.x-dev"
        }
    }
}

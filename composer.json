{
    "name": "ise/php-metapackage-development",
    "description": "Development metapackage for PHP used by Internalsystemerror Limited",
    "type": "metapackage",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/internalsystemerror/php-metapackage-development",
    "require": {
        "php": "^5.6 || ^7.0",
        "roave/security-advisories": "dev-master",
        "sensiolabs/security-checker": "^3.0",
        "phpunit/phpunit": "^5.7 || ^6.0",
        "squizlabs/php_codesniffer": "^2.7",
        "phpmd/phpmd": "^2.4",
        "sebastian/phpcpd": "^2.0",
        "phploc/phploc": "^3.0",
        "theseer/phpdox": "^0.8 || ^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.1.x-dev"
        }
    }
}

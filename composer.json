{
    "name": "ise/php-metapackage-development",
    "description": "Development metapackage for PHP used by Internalsystemerror Limited",
    "type": "metapackage",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/internalsystemerror/php-metapackage-development",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "classmap-authoritative": true,
        "discard-changes": true,
        "optimize-autoloader": true,
        "prepend-autoloader": true,
        "process-timeout": 5000,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.2",
        "phploc/phploc": "^5.0.0",
        "phpmd/phpmd": "^2.6.0",
        "phpstan/phpstan-phpunit": "^0.11.0",
        "phpstan/phpstan-strict-rules": "^0.11.0",
        "phpunit/phpunit": "^8.1.3",
        "roave/security-advisories": "dev-master",
        "sebastian/phpcpd": "^4.1.0",
        "squizlabs/php_codesniffer": "^3.4.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev",
            "dev-develop": "2.3.x-dev"
        }
    }
}
